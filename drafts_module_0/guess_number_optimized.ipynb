    "number = 4\n",
    "left = 1\n",
    "right = 100\n",
    "count = 1\n",
    "predict = 50\n",
    "while predict != number:\n",
    "    count+=1\n",
    "    predict = (left+right)//2\n",
    "    if predict > number:\n",
    "        right = predict\n",
    "    elif predict < number:\n",
    "        left = predict + 1    \n",
    "print(count)    "
